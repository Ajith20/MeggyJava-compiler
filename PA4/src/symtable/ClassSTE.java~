package symtable;

import symtable.Scope;

public class ClassSTE extends STE
{
	
	public boolean mMain;
	public String mSuperClass;
	public Scope mScope;
	public ClassSTE(String mName, boolean mMain, String mSuperClass, Scope mScope)
	{
		this.mName = mName;
		this.mMain = mMain;
		this.mSuperClass = mSuperClass;
		this.mScope = mScope;
		
	}
}
